<?xml version="1.0" encoding="UTF-8"?> 

<launch>    

  <!--- Run AMCL -->
  <include file="$(find try2)/2dnav/launch/amcl_1.launch" />

  <!-- Define your move_base node -->
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find try2)/2dnav/config/costmap_common_params.yaml" command="load" ns="global_costmap"/>
    <rosparam file="$(find try2)/2dnav/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find try2)/2dnav/config/local_costmap_params.yaml"  command="load" />
    <rosparam file="$(find try2)/2dnav/config/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find try2)/2dnav/config/move_base_params.yaml" command="load" />

    <remap from="map" to="map" />
  </node>

</launch>
