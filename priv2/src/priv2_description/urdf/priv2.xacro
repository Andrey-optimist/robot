      <inertia
        ixx="0.000966667"
        ixy="0"
        ixz="0"
        iyy="0.001133333"
        iyz="0"
        izz="0.000433333" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" /> <!--0.01 0.1 -0.0025 -->
      <geometry> 
        <mesh
          filename="package://priv2/src/priv2_description/meshes/base_link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://priv2/src/priv2_description/meshes/base_link.STL" />
      </geometry>
    </collision>
  </link>
 

 <link   name="wheel_r">
