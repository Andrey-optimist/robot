<?xml version="1.0" encoding="utf-8"?>

<robot name="priv2" xmlns:xacro="http://www.ros.org/wiki/xacro">


  <xacro:include filename="$(find priv2)/src/priv2_description/urdf/priv2.gazebo" />
  <xacro:include filename="$(find priv2)/src/priv2_description/urdf/materials.xacro" />
  <xacro:include filename="$(find priv2)/src/priv2_description/urdf/macros.xacro" />

  <link name="base_footprint"/>

  <joint name="base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
  </joint>


 <link  name="base_link">
    <inertial>
      <origin
        xyz="-0.025 0 0"
        rpy="0 0 0" />
      <mass      value="1" />
      <inertia
        ixx="0.000966667"
        ixy="0"
        ixz="0"
        iyy="0.001133333"
        iyz="0"
        izz="0.000433333" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" /> 
      <geometry> 
        <mesh
          filename="package://priv2/src/priv2_description/meshes/base_link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://priv2/src/priv2_description/meshes/base_link.STL" />
      </geometry>
    </collision>
  </link>
 

 <link   name="wheel_r">
    <inertial>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <mass
        value="0.5" />
      <inertia
        ixx="5.42E-06"
        ixy="0"
        ixz="0"
        iyy="5.42E-06"
        iyz="0"
        izz="0.00001" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="1.57 0 0" /> 
      <geometry>
        <mesh
          filename="package://priv2/src/priv2_description/meshes/wheel_r.STL" />
      </geometry>
      <material
        name="brown">
       
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="1.57 0 0" />
      <geometry>
        <mesh
          filename="package://priv2/src/priv2_description/meshes/wheel_r.STL" />
      </geometry>
    </collision>

  </link>


   <joint
    name="Joint_wheel_r"
    type="continuous">
    <origin
      xyz="-0.025 0.03 0.015"
      rpy="-1.5708 0 0" />
    <parent
      link="base_link" />
    <child
      link="wheel_r" />
    <axis
      xyz="0 0 -1" />

  </joint>

  <link   name="wheel_l">
    <inertial>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <mass
        value="0.5" />
      <inertia
        ixx="5.42E-06"
        ixy="0"
        ixz="0"
        iyy="5.42E-06"
        iyz="0"
        izz="0.00001" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0" 
        rpy="1.57 0 0" /> <!--0.0375 -0.0175 -0.13 -->
      <geometry>
        <mesh
          filename="package://priv2/src/priv2_description/meshes/wheel_l.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"  
        rpy="1.57 0 0" /> 
      <geometry>
        <mesh
          filename="package://priv2/src/priv2_description/meshes/wheel_l.STL" />
      </geometry>
    </collision>
 
  </link>


  <joint
    name="Joint_wheel_l"
    type="continuous">
   <origin
      xyz="-0.025 -0.03 0.015"
      rpy="1.5708 0 0" />
    <parent
      link="base_link" />
    <child
      link="wheel_l" />
    <axis
      xyz="0 0 1" />

  </joint>

 

 <link
    name="wheel_b_axis">
    <inertial>
      <origin
        xyz="-3.4694E-18 -0.0035 -1.7347E-18"
        rpy="0 0 0" />
      <mass
        value="0.5" />
      <inertia
        ixx="2.7941E-08"
        ixy="0"
        ixz="0"
        iyy="4.6383E-08"
        iyz="0"
        izz="2.3027E-08" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://priv2/src/priv2_description/meshes/axis_b.STL" />
      </geometry>
      <material
