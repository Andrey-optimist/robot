      <geometry>
        <mesh
          filename="package://priv2/src/priv2_description/meshes/axis_b.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="Joint_wheel_b"
    type="continuous">
    <origin
      xyz="0.025024 0 0"
      rpy="0 0 0" />
    <parent
      link="base_link" />
    <child
      link="wheel_b_axis" />
    <axis
      xyz="0 0 1" />
  </joint>
  <link
    name="wheel_b">
    <inertial>
      <origin
        xyz="0 -0.0009 0"
        rpy="0 0 0" />
      <mass
        value="0.1" />
   <inertia
        ixx="4.3881E-08"
        ixy="0"
        ixz="0"
        iyy="5.0048E-08"
        iyz="0"
        izz="5.0048E-08" />
    </inertial> 
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://priv2/src/priv2_description/meshes/wheel_b.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://priv2/src/priv2_description/meshes/wheel_b.STL" />
      </geometry>
    </collision>

  </link>

  <joint
    name="Joint_wheel_b_axis"
    type="continuous">
    <origin
      xyz="0.007 -0.0009 0.002"
      rpy="0 0 1.5708" />
    <parent
      link="wheel_b_axis" />
    <child
      link="wheel_b" />
    <axis
      xyz="1 0 0" />
  </joint>

 <!-- hokuyo -->
  <link name="hokuyo">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
    <box size="0.01 0.01 0.01"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://priv2/src/priv2_description/meshes/hokuyo.dae" scale="0.1 0.1 0.1"/>
      </geometry>
    </visual>

    <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
  </link>

    <joint name="hokuyo_joint" type="fixed">
    <axis xyz="0 1 0" />
    <origin xyz="-0.04 0 0.0295" rpy="0 0 3.14"/>
    <parent link="base_link"/>
    <child link="hokuyo"/>
  </joint>


</robot>
